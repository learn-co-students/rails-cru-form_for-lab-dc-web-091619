
<%= form_for @song do |f| %>

  <%= f.label :name, "Song Title:" %><br>
  <%= f.text_field :name %><br>

  <br>

  <%= f.label :artist_id, "Artist Name:" %><br>
  <%= f.text_field  :artist_id %><br>

  <br>

  <%= f.label :genre_id, "Genre:" %><br>
  <%= f.number_field  :genre_id %><br>
  <%#= f.collection_select :genre_id, Genre.all, :id, :name%>

  <br>

  <%= f.submit %>

<% end %>

 <button> <%= link_to "Back to All of the Artists" %> </button>
